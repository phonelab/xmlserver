<?xml version="1.0" encoding="UTF-8"?><manifest>
    <id meid="A000002A2804A8"/>
    <source name="phonelab" version="1" base="http://ps1.phonelab.org/download/">
    </source>

    <default asyncappdir="/mnt/sdcard/phonelab/apps/" hiddenappdir="/mnt/sdcard/phonelab/hidden/apps/">
    </default>
    
    <dumpservices>
    	<service name="battery"/>
    	<service name="batteryinfo"/>
        <service name="cpuinfo"/>
        <service name="location"/>
        <service name="meminfo"/>
        <service name="netstats"/>
        <service name="wifi"/>
        <service name="usagestats"/>
    </dumpservices>

    <statusmonitor>
      <parameter name="monitorInterval" value="10" units="min" setby="server"/>
      <parameter name="locationInterval" value="10" units="min" setby="server"/>
      <parameter name="dumpInterval" value="1" units="min" setby="server"/>
    </statusmonitor>
    
  </manifest>
